{
    "name": "aptof/slim-auth",
    "description": "Authentication Starter: Built with Slim, RedBean, Tailwind, Alpinejs...",
    "license": "MIT",
    "type": "project",
    "require": {
        "slim/psr7": "^1.3",
        "slim/slim": "4.*",
        "vlucas/phpdotenv": "^5.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "prefer-stable": true
}
